set term postscript eps enh color dashed "Helvetica" 18

set xdata time
set timefmt '%m-%d-%Y %H:%M:%S'

set xrange ["@PSTART@": "@PEND@"] noreverse
set format x "%m/%d\n%H:%M"

unset xlabel

set ylabel "Discharge, kcfs"
set format y "%.3g"
set ytics mirror

set arrow 1 from first "@NOW@", graph 0 to first "@NOW@", graph 1 nohead lt 7

set key below
set timestamp 

set output "RingoldDischarge.eps"
plot 'ts1286.out' using 1:($4/1000.0) title "Ringhold Hatchery (RM 354.8)" with lines lt 1 , \
     'ts11.out' using 1:($4/1000.0) title "Priest Rapids Dam (RM 397.1)" with lines lt 3
set output "100-FDischarge.eps"
plot 'ts1193.out' using 1:($4/1000.0) title "100-F Area (RM 368.9)" with lines lt 1 , \
     'ts11.out' using 1:($4/1000.0) title "Priest Rapids Dam (RM 397.1)" with lines lt 3
set output "100-DDischarge.eps"
plot 'ts1135.out' using 1:($4/1000.0) title "100-D Area (RM 377.7)" with lines lt 1 , \
     'ts11.out' using 1:($4/1000.0) title "Priest Rapids Dam (RM 397.1)" with lines lt 3
set output "100-BDischarge.eps"
plot 'ts194.out' using 1:($4/1000.0) title "100-D Area (RM 384.1)" with lines lt 1 , \
     'ts11.out' using 1:($4/1000.0) title "Priest Rapids Dam (RM 397.1)" with lines lt 3
set output
