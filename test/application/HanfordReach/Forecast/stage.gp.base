set term postscript eps enh color dashed "Helvetica" 18

set rmargin 7

set xdata time
set timefmt '%m-%d-%Y %H:%M:%S'

set xrange ["@PSTART@": "@PEND@"] noreverse
set format x "%d%b\n%H:%M"
set grid xtics

unset xlabel

set ylabel "Stage, ft"
set y2label "Stage, ft at Priest Rapids Dam"
set format y "%.0f"
set format y2 "%.0f"
set ytics nomirror
set y2tics nomirror
set grid ytics

set autoscale y
set autoscale y2

set arrow 1 from first "@NOW@", graph 0 to first "@NOW@", graph 1 nohead lt 7

set key right font "Helvetica,14"
set timestamp font "Helvetica,14"

set output "300-Stage.eps"
